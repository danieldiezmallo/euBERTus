#!/home/ddiezmallo/Projects/euBERTus/.pyvenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'runipy==0.1.5','console_scripts','runipy'
__requires__ = 'runipy==0.1.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('runipy==0.1.5', 'console_scripts', 'runipy')()
    )
